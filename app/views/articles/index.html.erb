<h1 align="center"> Listing all articles </h1>
<div align="center">
    <%= will_paginate @articles, 
                    renderer: Bootstrap5Renderer,
                    previous_label: '&laquo;',
                    next_label: '&raquo;',
                    inner_window: 1,
                    outer_window: 0,
                    class: 'pagination justify-content-center' %>
</div>                    

<%= render 'article', obj: @articles %>

<div align="center">
    <%= will_paginate @articles, 
                    renderer: Bootstrap5Renderer,
                    previous_label: '&laquo;',
                    next_label: '&raquo;',
                    inner_window: 1,
                    outer_window: 0,
                    class: 'pagination justify-content-center' %>
</div>    
